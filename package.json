{
  "name": "jekyll-arabica-boilerplate",
  "version": "1.0.0",
  "description": "A really simple project that serves as a boiler plate for developers building front end websites with Jekyll.",
  "main": "index.js",
  "scripts": {
    "start": "bundle exec jekyll serve --watch",
    "devhost": "bundle exec jekyll serve --watch --host=0.0.0.0",
    "hotreload": "bundle exec jekyll serve --livereload --host=0.0.0.0",
    "build": "bundle exec jekyll build"
  },
  "author": {
    "name": "Sricharan Krishnan (https://github.com/sricharankrishnan)",
    "email": "sricharan.krishnan@gmail.com",
    "url": "https://sricharankrishnan.github.io/"
  },
  "keywords": [
    "jekyll",
    "jekyll boilerplate",
    "jekyll arabica boilerplate",
    "jekyll websites"
  ]
}
